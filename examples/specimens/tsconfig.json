{
	"compilerOptions": {
		"target": "esnext",
		"jsx": "react-jsx",
		"jsxImportSource": "theme-ui",
		"lib": ["dom", "esnext"],
		"module": "esnext",
		"moduleResolution": "node",
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true
	},
	"include": ["./src/**/*", "./gatsby-node.ts", "./plugins/**/*"]
}
